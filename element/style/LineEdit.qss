LineEdit {
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #dcdfe6;
    color: #606266;
    font-size: inherit;
    height: 40px;
    line-height: 40px;
    padding: 0 15px;
    width: 100%;
}
LineEdit:hover {
    border-color: #c0c4cc;
}

LineEdit:focus {
    border-color: #409eff;
}
LineEdit:disabled {
    background-color: #f5f7fa;
    border-color: #e4e7ed;
    color: #c0c4cc;
}

